	<script type="text/javascript">
		$(document).ready(function() {
			var EditorWrap = $('#editorpick');
			var EditorWrap_ul = EditorWrap.find(".wrap342GR3 .photoList .editorspick");
			var EditorWrap_li = EditorWrap_ul.find("li").length * 395;
			EditorWrap_ul.css("width", EditorWrap_li);
			EditorWrap.find('.wrap342GR3 .photoList .btn.editor').on('click',
				function(e) {
					var me = $(this);
					var ul = me.siblings('ul.editorspick');
					var width = ul.width();
					left = Math.abs(parseInt(ul.css('margin-left')));
					move = 395;
					if (me.hasClass('btnRight')) {
						if (ul.is(':not(.animate)')) {
							ul.addClass('animate').animate({marginLeft : ((left+ move < width) ? ('-=' + move): '0')+ 'px'},
											{duration : 500,complete : function() {ul.removeClass('animate');}});
						}
					} else {
						if (ul.is(':not(.animate)')) {
							ul.addClass('animate').animate(
									{marginLeft : ((left - move >= 0) ? ('+=' + move) : ('-=' + Math.floor((width - 1) / move) * move))+ 'px'},
									{duration : 500, complete : function() {ul.removeClass('animate');}
									});
						}
					}
					e.preventDefault();
				});
		});
	</script>
